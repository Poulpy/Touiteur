<h3>Users</h3>



<ul class="list-group col-4">
  <% @users.each do |user| %>
    <li class="list-group-item">
      <%= user.email %>
      <div class="pull-right">
        <%= link_to "Delete", admin_user_path(user.id), method: :delete, class: "btn btn-sm btn-outline-danger" %>
      </div>
    </li>
    <br />
  <% end %>
</ul>
