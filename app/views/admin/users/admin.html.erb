<%# admin.html.erb %>

<ul class="list-group col-4">
  <% @users.each do |user| %>
    <li class="list-group-item">
      <%= user.email %>
      <div class="pull-right">
        <%= link_to "Unfollow", user, method: :delete, class: "btn btn-sm btn-outline-danger" %>
      </div>
    </li>
    <br />
  <% end %>
</ul>
